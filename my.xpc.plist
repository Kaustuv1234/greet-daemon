<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>my.greet.daemon</string>
		<key>ProgramArguments</key>
		<array>
			<string>/path/to/Go/binary</string>
			<string>-debug</string>
		</array>
		<key>MachServices</key>
		<dict>
			<key>my.xpc.server</key>
			<true/>
		</dict>
		<key>UserName</key>
		<string>root</string>
		<key>GroupName</key>
		<string>wheel</string>
		<key>RunAtLoad</key>
		<true/>
		<key>KeepAlive</key>
		<true/>
	</dict>
</plist>
